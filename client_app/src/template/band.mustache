<div class="row">
  <div class="span5">
    <h1>{{ band_name }}&nbsp;<small>{{status}}</small></h1>
  </div>
  <div class="span1 offset6">
    <span>#3 | Rock, Ru</span>
  </div>
</div>
<div class="row">
  <div class="span6">
    <span class="label">
      {{ primary_genre }}
    </span>
    {{#genres}}
      &nbsp;
      <span class="label">
        {{.}}
      </span>
    {{/genres}}
  </div>
</div>
<div class="row"><div class="span12">&nbsp;</div></div>
<div class="row">
  <div class="span12">
    <form class="form-inline">
      <button class="btn">offer a show</button>
      <button class="btn">offer blow job</button>
      <button class="btn">become a fan</button>
    </form>
  </div>
</div>
<hr>
<div class="row">
  <!-- main content -->
  <div class="span8">
    <div class="row">
      <div class="span4">
        <img src="http://placehold.it/370x250&text=main+pic" />
      </div>
      <div class="span4">
        <img src="http://placehold.it/370x250&text=music" />
      </div>
    </div>
    <hr>
    <div class="row">
      <div class="span4">
        <div class="well">
          <h5>Band bio</h5>
          <p>{{ bio }}</p>
        </div>
        <div class="well">
          <h5>Members</h5>
          {{#members}}
            <p>{{.}}</p>
          {{/members}}
        </div>
      </div>
      <div class="span4">
        <img src="http://placehold.it/370x250&text=video" />
      </div>
    </div>
  </div>
  <!-- news -->
  <div class="span4">
    <img src="http://placehold.it/370x540&text=news+block"/>
  </div>
</div>
